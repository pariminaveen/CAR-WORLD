<h1>Listing ueses</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
    <th>Image url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ueses.each do |uese| %>
  <tr>
    <td><%= uese.title %></td>
    <td><%= uese.description %></td>
    <td><%= uese.price %></td>
    <td><%= image_tag(uese.image_url, :width=>75, :height=>75) %></td>
    <td><%= link_to 'Show', uese %></td>
    <td><%= link_to 'Edit', edit_uese_path(uese) %></td>
    <td><%= link_to 'Destroy', uese, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Uese', new_uese_path %>
